<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.wjw.laboratory.entity.Namelist" table="namelist">
		<id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="courseId" column="courseId"/>
        <property name="courseName" column="courseName"/>
        <property name="studentAccount" column="studentAccount"/>
        <property name="studentName" column="studentName"/>
        <property name="sex" column="sex"/>
        <property name="specialty" column="specialty"/>
        <property name="examGrade" column="examGrade"/>
        <property name="totalGrade" column="totalGrade"/>
	</class>
</hibernate-mapping>
	