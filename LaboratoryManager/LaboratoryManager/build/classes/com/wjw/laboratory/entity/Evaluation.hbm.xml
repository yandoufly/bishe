<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.wjw.laboratory.entity.Evaluation" table="evaluation">
		<id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="courseId" column="courseId"/>
        <property name="studentId" column="studentId"/>
        <property name="answer1" column="answer1" length="6" />
        <property name="answer2" column="answer2" length="6" />
        <property name="answer3" column="answer3" length="6" />
        <property name="answer4" column="answer4" length="6" />
        <property name="answer5" column="answer5" length="6" />
        <property name="answer6" column="answer6" length="6" />
        <property name="suggestion" column="suggestion"/>
	</class>
</hibernate-mapping>
	